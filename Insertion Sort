package test;
class InsertionSort{
public static void main(String h[]){
int[] a = {22,9,8,44,99,123,44};
int i, j, temp;
    for (i = 1; i < a.length; i++)
    {
        j = i - 1;
        while (j >= 0 && a[j] < a[i] )
        {
            temp = a[i];
            a[i] = a[j];
            a[j] = temp;
            i=j;
            j--;

        }
    }
for(int x=0; x<a.length;x++){
    System.out.print(a[x] + " ");
    
  }System.out.println("");
}
}
